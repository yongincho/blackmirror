{
    "manifest_version": 2,
  
    "name": "example_chromeExtension",
    "description": "Black Mirror allows code detection in one tab",
    "version": "1.0.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "blackmirror.html"
    },
    "permissions": [
      "tabs",
      "<all_urls>"
    ]
  }